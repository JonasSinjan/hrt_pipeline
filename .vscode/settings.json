{
  "esbonio.server.enabled": true,
  "restructuredtext.confPath": "",
  "git.autofetch": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Material Theme Palenight High Contrast",
  "explorer.confirmDragAndDrop": false,
  "editor.tokenColorCustomizations": 
  {
      "comments": "#ffa600de",
      "textMateRules":[
          {
            "scope": "string.quoted.docstring.multi.python",
            "settings":{
              "foreground": "#ffa600de"
            }
          }, 
          {
              "scope": "string.quoted.docstring.multi.python punctuation.definition.string.end.python",
              "settings": {
                  "foreground": "#ffa600de"
              }
          },
          {
              "scope": "string.quoted.docstring.multi.python punctuation.definition.string.begin.python",
              "settings": {
                  "foreground": "#ffa600de"
              }
          },
          {
              "scope": "punctuation.definition.comment.python",
              "settings": {
                  "foreground": "#ffa600de"
              }
          }
        ]
  },
  "workbench.editorAssociations": {
      "*.ipynb": "jupyter-notebook"
  },
  "notebook.cellToolbarLocation": {
      "default": "right",
      "jupyter-notebook": "left"
  },
  "editor.wordWrap": "on"
}